{
  "name": "quorum-sql-server",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development tsx watch src/index.ts",
    "migrate": "node dist/migrate.js",
    "migrate:seed": "node dist/migrate.js --seed",
    "migrate:dev": "tsx src/migrate.ts",
    "smoke": "node dist/smoke.js",
    "smoke:dev": "tsx src/smoke.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.2.0",
    "@fastify/rate-limit": "^10.3.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.7.1",
    "jose": "^6.1.3",
    "pg": "^8.17.1",
    "prom-client": "^15.1.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.19.0",
    "@types/pg": "^8.15.5",
    "tsx": "^4.20.5",
    "typescript": "^5.8.3"
  }
}
